            .televzr-dwn-popup-os.mac-logo {
                background-image: url('data:image/svg+xml,%3Csvg width="13" height="13" viewBox="0 0 13 13" fill="none" xmlns="http://www.w3.org/2000/svg"%3E%3Cpath opacity=".7" d="M6.805.197c-.716.868-2.524 3.395-2.5 6.6h2.587l-.021.506L6.764 9.7c1.05-.121 2.116-.426 3.09-1.026l.463.837c-1.149.709-2.39 1.051-3.596 1.178l-.091 2.094-.917-.045.088-1.994c-2.793.051-5.162-.928-5.312-.99l-.37-.157.093-.406.199-.886.894.226-.103.46c.76.266 2.598.815 4.642.77l.085-1.996H3.425l-.028-.453c-.2-3.185 1.249-5.766 2.22-7.116h-7.914v12.606h14.7V.197H6.804zm1.483 3.879h-.759V2.642h.759v1.434zM1.418 2.6h.72v1.476h-.72V2.6z" fill="%23B8B7C0"/%3E%3C/svg%3E%0A')
            }

            .televzr-dwn-popup-os.win-logo {
                background-image: url('data:image/svg+xml,%3Csvg width="14" height="14" viewBox="0 0 14 14" fill="none" xmlns="http://www.w3.org/2000/svg"%3E%3Cpath opacity=".7" d="M.673 1.972l5.297-.72.002 5.094-5.294.03-.005-4.404zm5.295 4.961l.004 5.098-5.295-.726V6.9l5.29.034zm.642-5.775L13.633.136v6.145l-7.023.056V1.158zm7.025 5.823l-.002 6.117-7.023-.988-.01-5.14 7.035.01z" fill="%23B8B7C0"/%3E%3C/svg%3E')
